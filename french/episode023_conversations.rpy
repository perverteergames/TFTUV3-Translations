    # c "Love you, Thyia."
    c "Je t'aime, Thyia."

# game/episode023_conversations.rpy:1450
translate french ep023_thyia_talk_2ceadfe5:

    # th "Love you too, [p_name_short]."
    th "Je t'aime aussi, [p_name_short]."

# game/episode023_conversations.rpy:1453
